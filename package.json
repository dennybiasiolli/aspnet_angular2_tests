{
  "name": "angular2-aspnetcore-starter",
  "version": "0.0.1",
  "description": "Starter application using Angular 2 and ASP.NET Core",
  "main": "\"\"",
  "scripts": {
    "browser:chrome": "open -a Google\\ Chrome http://localhost:5000",
    "browser:firefox": "open -a Firefox http://localhost:5000",
    "browser:safari": "open -a Safari http://localhost:5000",
    "clean": "npm run clean:dotnet && npm run clean:npm",
    "clean:dotnet": "npm run clean:bin && npm run clean:obj",
    "clean:bin": "rm -rf bin",
    "clean:obj": "rm -rf obj",
    "clean:npm": "rm -rf node_modules",
    "install": "dotnet restore",
    "start": "tsc && concurrently \"tsc -w\" \"dotnet run\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dennybiasiolli/aspnet_angular2_tests.git"
  },
  "author": "Denny Biasiolli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dennybiasiolli/aspnet_angular2_tests/issues"
  },
  "homepage": "https://github.com/dennybiasiolli/aspnet_angular2_tests#readme",
  "dependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/http": "^2.4.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/router": "^3.4.1",
    "angular-in-memory-web-api": "^0.2.2",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.9",
    "rxjs": "5.0.1",
    "systemjs": "^0.19.41",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@types/node": "^6.0.53",
    "concurrently": "^3.1.0",
    "typescript": "^2.1.4"
  }
}
